#include <stdio.h>

int main() {
    int num1, num2, num3;

    printf("Введите три целых числа: ");
    if (scanf("%d %d %d", &num1, &num2, &num3) != 3) {
        printf("n/a");
        return 0;
    }

    if (num1 > 0) {
        num1 = num1 * num1 *num1;
    } else {
        num1 = 0;
    }

    if (num2 > 0) {
        num2 = num2 * num2 * num2;
    } else {
        num2 = 0;
    }

    if (num3 > 0) {
        num3 = num3 *num3 * num3;
    } else {
        num3 = 0;
    }

    printf("%d %d %d\n", num1, num2, num3);

    return 0;
}
