#include <stdio.h>

int main() {
    int arr[10];  

    for (int i = 0; i < 10; i++) {
        arr[i] = 7;  
    }

       printf("Массив, заполненный значениями '7': ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
Задание 6.4
#include <stdio.h>

int main() {
    int arr[10];  

    for (int i = 0; i < 10; i++) {
        if (i >= 5) {
            arr[i] = (i + 1) * (i + 1);  
        } else {
            arr[i] = 7;  
        }
    }

        printf("Массив, заполненный значениями: ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}


